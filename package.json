  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lucybot/api-spec-converter/issues"
  },
  "homepage": "https://github.com/lucybot/api-spec-converter",
  "dependencies": {
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^3.12.0",
    "@types/node": "^22.0.0",
    "typescript": "^5.0.0"
  }
